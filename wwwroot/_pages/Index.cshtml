@{
    ViewBag.visibleGrid = false;
    ViewBag.visibleBreakpoints = false;
    Layout = "~/Views/Layouts/bb.cshtml";
    var json = Helpers.ModelHelper.GetModel("project.json");
}

<div class="container">
	<header class="header" role="banner">
		<a href="#" class="logo">
			<img src="/_bb/bb-logo.svg" alt="Building Blocks" width="83" height="50" />
		</a>
		<h1 class="project-title">
			@json.title | @json.version
			@* <span class="last-modified">
				<b>Last Modified:</b> <span>{{{timestamp}}}</span>
			</span> *@
		</h1>
	</header>
	<div class="main" id="main" role="main">
		@Html.Partial("~/Views/Components/_bb/pageList.cshtml")
		@* {{>styleguide-link}} *@
	</div>
	<footer class="footer" role="contentinfo">
		<p>&copy; Copyright 2015 Building Blocks Ltd.</p>
	</footer>
</div>





